min_peers = 100
max_peers = 100
max_uploads = 100
download_rate = 0
upload_rate = 0
directory = /home/rtorrent/downloads
session = /home/rtorrent/.rtsession
schedule = watch_directory,10,10,load_start=/home/rtorrent/watch/*.torrent
port_range = 6881-6889
port_random = no
check_hash = yes
peer_exchange = yes
dht = auto
dht_port = 6881

scgi_local = /tmp/rpc.socket
schedule = chmod,0,0,"execute=chmod,777,/tmp/rpc.socket"